{
  "name": "amico",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build --emptyOutDir",
    "copy:textures": "cp -R textures ../amico-be/public/",
    "copy:mesh": "cp -R mesh ../amico-be/public/",
    "postbuild": "npm run copy:mesh && npm run copy:textures",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.8",
    "@types/lodash": "^4.14.177",
    "@types/page": "^1.11.5",
    "@types/three": "^0.134.0",
    "three-orbitcontrols": "^2.110.3",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "dashjs": "^4.1.0",
    "joro": "^1.0.4",
    "lodash": "^4.17.21",
    "nanohtml": "^1.9.1",
    "nanomorph": "^5.4.3",
    "obake": "^0.1.2",
    "obake.js": "^1.2.1",
    "page": "^1.11.6",
    "three": "^0.134.0"
  }
}
