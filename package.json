{
	"name": "amico",
	"version": "0.0.1",
	"description": "A node js chat app",
	"main": "backend/dist/server.js",
	"scripts": {
		"prestart": "npm run build:back && npm run build:front",
		"start": "node bundle/server.js",
		"deploy": "cd backend/dist/ && now",
		"bundle": "npm run build:back && npm run build:front",
		"bundle:front": "cd frontend && npm install && npm run build",
		"bundle:back": "cd backend && npm install && npm run build"
	},
	"dependencies": {
		"@thelevicole/youtube-to-html5-loader": "^4.0.1",
		"coffee-script": "*",
		"cors": "^2.8.5",
		"express": "^4.12.4",
		"fabricjs": "^1.0.0",
		"got": "^11.8.2",
		"node-fetch": "^3.1.0",
		"socket.io": "*",
		"typescript": "^4.4.4",
		"yt-live-url": "^1.0.1"
	},
	"engines": {
		"node": "12.x"
	},
	"keywords": [
		"node",
		"hyperdev",
		"express",
		"chat"
	],
	"license": "MIT",
	"devDependencies": {
		"now": "^21.0.1",
		"vercel": "^23.1.2"
	}
}
